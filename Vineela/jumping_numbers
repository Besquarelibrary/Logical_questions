#Jumping numbers
class Solution:

	def jumpingnums(self, x):
		for i in range(x, 0, -1):
			k = 0
			s = str(i)
			for j in range(len(s) - 1):
				if (int(s[j]) - int(s[j + 1])) ** 2 != 1:
					k = 1
			if k == 0:
				return i


def main():
	solution = Solution()
	result = solution.jumpingnums(8769)
	print(result)


if __name__ == '__main__':
	main()
