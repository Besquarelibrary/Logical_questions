#reverse the words in a string
# using split
v = "i love being simple"
z = v.split()
y = " ".join(z[::-1])
print(y)

# another method
s = "i can resilient myself"
y = []
s1 = ""
for i in s:
    if i == " ":
        y.append(s1)
        s1 = ""
    else:
        s1 = s1+i
y.append(s1)
result = " ".join(y[i] for i in range(-1, -len(y)-1, -1))
print(result)
