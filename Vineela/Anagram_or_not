#whether the given string is anagram or not
x = "vinni"
y = "inniv"
d1 = {}
d2 = {}
for i in x:
    if i in d1:
        d1[i] = d1[i]+1
    else:
        d1[i] = 1
print(d1)
for i in y:
    if i in d2:
        d2[i] = d2[i]+1
    else:
        d2[i] = 1
print(d2)
if d1 == d2:
    print("the given string is anagram")
else:
    print("the given string is not anagram")

