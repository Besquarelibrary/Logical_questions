#whether the given string is anagram or not
x = "vineela"
y = "aleeniv"
dict1 = {}
dict2 = {}
for i in x:
    if i in dict1:
        dict1[i] = dict1[i]+1
    else:
        dict1[i] = 1
print(dict1)
for i in y:
    if i in dict2:
        dict2[i] = dict2[i]+1
    else:
        dict2[i] = 1
print(dict2)
if dict1 == dict2:
    print("the given string is anagram")
else:
    print("the given string is not anagram")

