s = "Hi Team, the question is given by me & it should be optimizable code & the time complexity & space complexity should be equal or less than O(n)."
table = 2
s1 = ""
# this loop is for removing all the special characters and spaces in a given string
for i in s:
    if (ord(i) >= 97 and ord(i) <= 122) or (ord(i) >= 65 and ord(i) <= 90):
        s1 += i
print(s1)
#  to find the length of the given string
length = 0
for j in s1:
    length += 1
print(length)

# This loop is to multiply with the "table", Which is user input
for i in range(1, length+1):
    result = table * i

# comparing result and length
    if result < length:
        print(t,"*",i,"=" ,s1[result])
